{
  "schemaVersion": 1,
  "id": "iamod",
  "version": "${version}",

  "name": "Better AI Mod",
  "description": "This Mod works on AIs as well as adds stuff as demanded by Sam Stinson",
  "authors": [
    "Hugo Noublanche"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "MIT",
  "icon": "assets/iamod/icon.png",

  "environment": "*",
  "entrypoints": {
    "client": [
      "net.lemeow.iamod.IAModClientMod"
    ],
    "main": [
      "net.lemeow.iamod.IAMod"
    ]
  },
  "mixins": [
    "iamod.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.18.2",
    "java": ">=17",
    "bclib": "1.4.7"
  },
  "breaks": {
    "bclib": "<1.4.7"
  },
  "suggests": {
    "another-mod": "*"
  }
}
